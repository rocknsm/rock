services:
  - fsf

file_paths:
  - ['/opt/fsf/fsf-server/conf/config.py', 'fsf:fsf', '0644']
  - ['/opt/fsf/fsf-client/conf/config.py', 'fsf:fsf', '0644']
  - ['/opt/fsf/fsf-server/main.py', 'fsf:fsf', '0755']
  - ['/opt/fsf/fsf-client/fsf_client.py', 'fsf:fsf', '0755']
  - ['/var/lib/yara-rules/rules.yara', 'fsf:fsf', '0664']

dir_paths:
  - /data/fsf
  - /opt/fsf/fsf-server
  - /opt/fsf/fsf-client
  - /run/fsf
  - /var/lib/yara-rules/

packages:
  - fsf

listening_ports:
  - 5800